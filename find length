function stringLength(str) {
    return str.length;
}


console.log(stringLength("hello world")); // prints 11
console.log(stringLength("javascript")); // prints 8

function stringLength(str) {
    let count = 0;
    for(let i = 0; str.charAt(i) != ''; i++) {
        count++;
    }
    return count;
}
